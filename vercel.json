{
  "buildCommand": "uv pip install -r requirements.txt && python manage.py collectstatic --no-input",
  "outputDirectory": "staticfiles",
  "rewrites": [
    { "source": "/(.*)", "destination": "sisaket/wsgi.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "sisaket.settings",
    "PYTHONUNBUFFERED": "1"
  }
}

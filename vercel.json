{
  "builds": [
    {
      "src": "sisaket/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "sisaket.settings"
    },
    "commands": [
      "pip install -r requirements.txt",
      "python manage.py collectstatic --no-input"
    ]
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "sisaket/wsgi.py"
    }
  ],
  "static": {
    "staticfiles": "/static/"
  }
}
